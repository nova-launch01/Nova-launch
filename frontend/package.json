{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "ANALYZE=true vite build",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest run src/test/integration --config vitest.config.ts",
    "test:integration:watch": "vitest src/test/integration --config vitest.config.ts",
    "test:integration:coverage": "vitest run src/test/integration --coverage --config vitest.config.ts",
    "test:e2e": "vitest run src/test/e2e --config vitest.config.ts",
    "test:e2e:watch": "vitest src/test/e2e --config vitest.config.ts",
    "test:a11y": "vitest run src/test/accessibility --config vitest.config.ts",
    "test:a11y:watch": "vitest src/test/accessibility --config vitest.config.ts",
    "test:a11y:coverage": "vitest run src/test/accessibility --coverage --config vitest.config.ts",
    "test:a11y:report": "pa11y --reporter json --reporter csv http://localhost:5173",
    "test:a11y:ci": "vitest run src/test/accessibility --config vitest.config.ts --reporter=junit --outputFile=test-results/junit-a11y.xml"
  },
  "dependencies": {
    "@sentry/browser": "^10.39.0",
    "@stellar/freighter-api": "^6.0.1",
    "@stellar/stellar-sdk": "^13.3.0",
    "i18next": "^25.8.12",
    "lucide-react": "^0.575.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.4",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.6.4",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.10.13",
    "@types/pa11y": "^5.3.7",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "axe-core": "^4.11.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fast-check": "^4.5.3",
    "globals": "^16.5.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^28.0.0",
    "pa11y": "^9.1.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "5.11.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.1.8",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.18"
  }
}
